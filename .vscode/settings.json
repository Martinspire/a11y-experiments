{
  "auto-rename-tag.activationOnLanguage": [
    "html",
    "xml",
    "php"
  ],
  "coverage-gutters.coverageFileNames": [
    "lcov.info",
    "ADDFILENAMEHERE.xml",
    "coverage.xml"
  ],
  "coverage-gutters.showRulerCoverage": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.cursorBlinking": "smooth",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.stickyScroll.enabled": false,
  "editor.stickyTabStops": true,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "eslint.format.enable": true,
  "eslint.ignoreUntitled": true,
  "eslint.lintTask.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "html"
  ],
  "explorerExclude.backup": {},
  "explorer.fileNesting.enabled": true,
  "explorer.incrementalNaming": "smart",
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.exclude": {
    "**/.angular": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.gitignore": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.npmrc": true,
    "**/.nx": true,
    "**/.svn": true,
    "**/.vscode": true,
    "**/__MACOSX": true,
    "**/CVS": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/Thumbs.db": true,
    ".eslintignore": true,
    ".eslintrc.json": true,
    ".gitignore": true,
    ".prettierignore": true,
    ".prettierrc": true,
    ".stylelintrc.json": true,
    "coverage": true,
    "e2e/.eslintrc.json": true,
    "e2e/playwright.config.ts": true,
    "e2e/project.json": true,
    "e2e/tsconfig.json": true,
    "jest.config.ts": true,
    "jest.preset.js": true,
    "LICENSE": true,
    "nx.json": true,
    "package-lock.json": true,
    "package.json": true,
    "project.json": true,
    "src/assets": true,
    "src/environments": true,
    "src/favicon.ico": true,
    "src/main.ts": true,
    "src/test-setup.ts": true,
    "tmp": true,
    "tsconfig.app.json": true,
    "tsconfig.editor.json": true,
    "tsconfig.json": true,
    "tsconfig.spec.json": true
  },
  "files.trimTrailingWhitespace": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "search.actionsPosition": "auto",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "sortJSON.contextMenu": {
    "sortJSON": false,
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONType": false,
    "sortJSONTypeReverse": false,
    "sortJSONValues": false,
    "sortJSONValuesReverse": false
  },
  "sortJSON.excludedFiles": [
    "package.json"
  ],
  "typescript.format.semicolons": "insert",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  "vsicons.dontShowNewVersionMessage": true,
  "window.openFoldersInNewWindow": "on",
  "workbench.tips.enabled": false
}
